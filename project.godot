[display]

window/stretch/mode="viewport"

[dotnet]

project/assembly_name="ProjectDaybreakGD4"

[editor_plugins]

enabled=PackedStringArray("res://addons/clyde/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg")

[filesystem]

import/blender/enabled=false

